{
  "design": {
    "name": "SaaS Illustration Template",
    "source": "https://saas-illustration.codedesign.app/",
    "colors": {
      "primary": "#6366f1",
      "primaryDark": "#4f46e5",
      "primaryLight": "#818cf8",
      "secondary": "#10b981",
      "secondaryDark": "#059669",
      "accent": "#f59e0b",
      "accentDark": "#d97706",
      "neutral": {
        "50": "#f8fafc",
        "100": "#f1f5f9",
        "200": "#e2e8f0",
        "300": "#cbd5e1",
        "400": "#94a3b8",
        "500": "#64748b",
        "600": "#475569",
        "700": "#334155",
        "800": "#1e293b",
        "900": "#0f172a"
      },
      "success": "#22c55e",
      "warning": "#f59e0b",
      "error": "#ef4444",
      "background": {
        "primary": "#ffffff",
        "secondary": "#f8fafc",
        "tertiary": "#f1f5f9"
      }
    },
    "typography": {
      "fontFamily": {
        "primary": "'Inter', -apple-system, BlinkMacSystemFont, 'Segoe UI', sans-serif",
        "heading": "'Cal Sans', 'Inter', sans-serif",
        "mono": "'JetBrains Mono', 'Fira Code', monospace"
      },
      "fontSize": {
        "xs": "0.75rem",
        "sm": "0.875rem",
        "base": "1rem",
        "lg": "1.125rem",
        "xl": "1.25rem",
        "2xl": "1.5rem",
        "3xl": "1.875rem",
        "4xl": "2.25rem",
        "5xl": "3rem",
        "6xl": "3.75rem"
      }
    },
    "buttons": {
      "primary": {
        "backgroundColor": "#4F46E5",
        "color": "#FFFFFF",
        "padding": "0.75rem 1.5rem",
        "borderRadius": "0.375rem",
        "hover": {
          "backgroundColor": "#4338CA"
        }
      },
      "secondary": {
        "backgroundColor": "#FFFFFF",
        "color": "#4F46E5",
        "border": "1px solid #D1D5DB",
        "padding": "0.75rem 1.5rem",
        "borderRadius": "0.375rem",
        "hover": {
          "backgroundColor": "#F9FAFB"
        }
      },
      "accent": {
        "backgroundColor": "#10B981",
        "color": "#FFFFFF",
        "padding": "0.75rem 1.5rem",
        "borderRadius": "0.375rem",
        "hover": {
          "backgroundColor": "#0D9F6E"
        }
      }
    },
    "layout": {
      "containerWidth": "1280px",
      "sectionPadding": "5rem 2rem",
      "gridGap": "2rem",
      "borderRadius": {
        "small": "0.375rem",
        "medium": "0.5rem",
        "large": "1rem"
      },
      "boxShadow": "0 4px 6px -1px rgba(0, 0, 0, 0.1), 0 2px 4px -1px rgba(0, 0, 0, 0.06)"
    },
    "components": {
      "navbar": {
        "height": "80px",
        "backgroundColor": "#FFFFFF",
        "boxShadow": "0 1px 3px 0 rgba(0, 0, 0, 0.1), 0 1px 2px 0 rgba(0, 0, 0, 0.06)",
        "logo": {
          "color": "#4F46E5",
          "size": "1.5rem",
          "weight": "700"
        },
        "links": {
          "color": "#4B5563",
          "hoverColor": "#4F46E5",
          "activeColor": "#4F46E5"
        }
      },
      "heroSection": {
        "background": "linear-gradient(to right, #F9FAFB, #FFFFFF)",
        "contentAlignment": "center",
        "illustrationStyle": "flat colorful illustrations with abstract shapes"
      },
      "features": {
        "card": {
          "backgroundColor": "#FFFFFF",
          "padding": "2rem",
          "borderRadius": "0.5rem",
          "boxShadow": "0 1px 3px 0 rgba(0, 0, 0, 0.1), 0 1px 2px 0 rgba(0, 0, 0, 0.06)",
          "icon": {
            "backgroundColor": "#E0E7FF",
            "color": "#4F46E5",
            "size": "2.5rem",
            "borderRadius": "0.75rem"
          }
        }
      },
      "testimonials": {
        "backgroundColor": "#1F2937",
        "textColor": "#FFFFFF",
        "card": {
          "backgroundColor": "#374151",
          "padding": "1.5rem",
          "borderRadius": "0.5rem"
        }
      },
      "cta": {
        "background": "linear-gradient(to right, #4F46E5, #7C3AED)",
        "textColor": "#FFFFFF",
        "button": {
          "backgroundColor": "#FFFFFF",
          "color": "#4F46E5"
        }
      }
    },
    "illustrations": {
      "style": "modern flat design with abstract shapes",
      "colorPalette": ["#4F46E5", "#10B981", "#F59E0B", "#EC4899"],
      "characteristics": {
        "roundedCorners": true,
        "simpleGeometricShapes": true,
        "gradients": false,
        "minimalDetails": true
      }
    },
    "spacing": {
      "xs": "0.5rem",
      "sm": "1rem",
      "md": "1.5rem",
      "lg": "2rem",
      "xl": "3rem",
      "2xl": "4rem"
    },
    "borderRadius": {
      "sm": "0.375rem",
      "md": "0.5rem",
      "lg": "0.75rem",
      "xl": "1rem",
      "2xl": "1.5rem"
    },
    "shadows": {
      "sm": "0 1px 2px 0 rgb(0 0 0 / 0.05)",
      "md": "0 4px 6px -1px rgb(0 0 0 / 0.1), 0 2px 4px -2px rgb(0 0 0 / 0.1)",
      "lg": "0 10px 15px -3px rgb(0 0 0 / 0.1), 0 4px 6px -4px rgb(0 0 0 / 0.1)",
      "xl": "0 20px 25px -5px rgb(0 0 0 / 0.1), 0 8px 10px -6px rgb(0 0 0 / 0.1)"
    }
  }
}